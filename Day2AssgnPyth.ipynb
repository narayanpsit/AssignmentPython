        "# Project: Lottery\n",
        "w = input('Enter the lottery word: ')\n",
        "\n",
        "fqcy = [0] * 26\n",
        "\n",
        "print(fqcy)\n",
        "\n",
        "for i in w:\n",
        "  fqcy[ord(i) - 97] += 1\n",
        "\n",
        "print(fqcy)\n",
        "y = fqcy.index(max(fqcy))\n",
        "print(y)\n",
        "print(chr(97 + y))"
      ]
